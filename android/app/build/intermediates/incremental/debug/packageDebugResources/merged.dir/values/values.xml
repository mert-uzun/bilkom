<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="black">#FF000000</color>
    <color name="purple_200">#FFBB86FC</color>
    <color name="purple_500">#FF6200EE</color>
    <color name="purple_700">#FF3700B3</color>
    <color name="teal_200">#FF03DAC5</color>
    <color name="teal_700">#FF018786</color>
    <color name="white">#FFFFFFFF</color>
    <string name="app_name">Bilkom</string>
    <string name="email_hint">Email</string>
    <string name="login">Login</string>
    <string name="login_failed">Login failed</string>
    <string name="network_error">Network error</string>
    <string name="password_hint">Password</string>
    <string name="register">Register</string>
    <string name="welcome_message">Welcome to Bilkom</string>
    <style name="Theme.Bilkom" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
    </style>
    <style name="Theme.Bilkom.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
    <style name="Theme.Bilkom.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.Bilkom.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>
    <style name="Widget.Bilkom.Button" parent="Widget.MaterialComponents.Button">
        <item name="cornerRadius">24dp</item>
        <item name="android:insetTop">12dp</item>
        <item name="android:insetBottom">12dp</item>
        <item name="android:paddingStart">32dp</item>
        <item name="android:paddingEnd">32dp</item>
    </style>
</resources>